{
  "name": "r-duelyst",
  "version": "1.0.0",
  "description": "Stylesheet for the Duelyst subreddit.",
  "scripts": {
    "build": "lessc src/main.less dist/main.css --clean-css='--s1 --advanced --compatibility=ie9' --autoprefix",
    "lint": "stylelint 'src/**/*.less' --syntax less",
    "css": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andolf/duelyst-subreddit.git"
  },
  "keywords": [
    "duelyst",
    "css",
    "less"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andolf/duelyst-subreddit/issues"
  },
  "homepage": "https://github.com/andolf/duelyst-subreddit#readme",
  "devDependencies": {
    "less": "^3.8.0",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "stylelint": "^8.1.1"
  }
}
